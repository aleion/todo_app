
<div class="container">
<div class="col-sm-6 well">
<h2>Tareas realizadas</h2>
    <% @completed.each do |c|%>
    <h5>• <%= c.description %></h5>
    <% end %>
</div>


<div class="col-sm-6 well">
<h2>Tareas no realizadas</h2>
    <% @uncompleted.each do |c|%>
    <h5>• <%= c.description %></h5>
    <% end %>
</div>
</div>